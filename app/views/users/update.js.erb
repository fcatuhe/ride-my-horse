<% if @user.errors.any? %>
  // Render new review form with errors
  $('#well').html("<%= j render 'form', user: @user %>");
<% else %>
  $('#well').html("<%= j render 'form', user: @user %>");
  $('#collapseProfile').collapse('hide');
  $('#my-profile').html("<%= j render 'user', user: @user %>");
<% end %>
