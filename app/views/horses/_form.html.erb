<%= simple_form_for [@user,@horse] do |f| %>
  <%= f.input :name %>
  <%= f.association :category %>
  <%= f.input :price %>
  <%= f.input :address %>
  <%= f.input :equipment %>
  <%= f.input :description %>
  <%= f.input :photo, as: :attachinary %>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>
